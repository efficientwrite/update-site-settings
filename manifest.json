{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Update Site settings",
  "description": "Set site settings per domain on mobile easily",
  "permissions": ["contentSettings", "activeTab", "declarativeContent"],
  "background": { "service_worker": "serviceWorker.js" },
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "incognito": "split",
  "icons": {
    "128": "assets/icon128.png",
    "48": "assets/icon48.png",
    "32": "assets/icon32.png",
    "64": "assets/icon64.png",
    "16": "assets/icon16.png"
  }
}
